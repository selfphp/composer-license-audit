{
  "name": "selfphp/composer-license-audit",
  "description": "Audit Composer dependencies for license compliance",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "symfony/console": "^7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "Selfphp\\ComposerLicenseAudit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Selfphp\\ComposerLicenseAudit\\Tests\\": "tests/"
    }
  },
  "bin": [
    "bin/license-audit"
  ]
}
