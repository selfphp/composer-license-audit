{
  "name": "selfphp/composer-license-audit",
  "description": "Automated license audit tool for Composer dependencies with blacklist and CI integration.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "composer",
    "license",
    "audit",
    "compliance",
    "ci",
    "blacklist",
    "php",
    "dependency"
  ],
  "authors": [
    {
      "name": "Damir Enseleit",
      "email": "damir.enseleit@selfphp.de",
      "homepage": "https://www.selfphp.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "symfony/console": "^7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "Selfphp\\ComposerLicenseAudit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Selfphp\\ComposerLicenseAudit\\Tests\\": "tests/"
    }
  },
  "bin": [
    "bin/license-audit"
  ]
}
